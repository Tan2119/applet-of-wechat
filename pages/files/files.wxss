.container {
  padding: 20rpx;
}

.file-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  margin-bottom: 20rpx;
  padding: 20rpx;
  border-radius: 12rpx;
}

.file-icon {
  font-size: 40rpx;
}

.file-info {
  flex: 1;
  margin-left: 20rpx;
}

.time {
  font-size: 24rpx;
  color: #999;
}
